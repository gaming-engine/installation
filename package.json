{
    "name": "gaming-engine-installation",
    "version": "1.0.0",
    "description": "Installer for the Gaming Engine",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:coverage:watch": "jest --coverage --watch",
        "prepare": "husky install",
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gaming-engine/installation.git"
    },
    "keywords": [
        "gaming-engine"
    ],
    "author": "Andrew Judd",
    "license": "MIT",
    "bugs": {
        "url": "https://gitlab.com/gaming-engine/installation/issues"
    },
    "homepage": "https://gitlab.com/gaming-engine/installation#readme",
    "devDependencies": {
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^12.1.4",
        "@vue/compiler-sfc": "^3.2.8",
        "@vue/test-utils": "^2.0.0-rc.13",
        "autoprefixer": "^10.3.1",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.24.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^7.16.0",
        "husky": "^7.0.0",
        "jest": "^27.1.0",
        "jest-cli": "^27.1.0",
        "jest-serializer-vue": "^2.0.2",
        "laravel-mix": "^6.0.49",
        "lint-staged": "^11.0.0",
        "postcss": "^8.3.6",
        "prettier": "^2.3.2",
        "prettier-eslint-cli": "^5.0.1",
        "vue-loader": "^16.5.0",
        "vue3-jest": "^27.0.0-alpha.1"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "lodash": "^4.17.19",
        "moment": "^2.29.4",
        "moment-timezone": "^0.5.35",
        "vue": "^3.2.2"
    }
